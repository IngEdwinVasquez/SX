<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="ticket_pos.OrderReceipt" t-inherit="point_of_sale.OrderReceipt" t-inherit-mode="extension">
        <xpath expr="//ReceiptHeader" position="after">
            <CustomerData />
        </xpath>
        <xpath expr="//div[hasclass('pos-receipt')]" position="inside">
            <ReceiptExtras />
        </xpath>
    </t>
</templates>
